#include <bits/stdc++.h>
using namespace std;

int arr1[500001] = { 0, };
int arr2[500001] = { 0, };
int n, m;


int main()
{
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> arr1[i];
    }

    cin >> m;
    for (int i = 0; i < m; i++) {
        cin >> arr2[i];
    }
    sort(arr1, arr1 + n);

    for (int i = 0; i < m; i++) {
        cout << binary_search(arr1, arr1 + n, arr2[i]) << " ";
    }

    return 0;
}
